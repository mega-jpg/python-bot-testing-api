{
  "name": "kjc-python-bot-testing-api",
  "version": "2.0.0",
  "description": "FastAPI + Static Frontend with ESLint and Prettier",
  "main": "static/js/script.js",
  "scripts": {
    "lint": "eslint static/js/**/*.js",
    "lint:fix": "eslint static/js/**/*.js --fix",
    "format": "prettier --write static/**/*.{js,css,html}",
    "format:check": "prettier --check static/**/*.{js,css,html}",
    "dev": "python main.py",
    "setup": "npm install"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "@eslint/js": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3"
  },
  "keywords": ["fastapi", "javascript", "eslint", "prettier", "python"],
  "author": "KJC Team",
  "license": "MIT"
}